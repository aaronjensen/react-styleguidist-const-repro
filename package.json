{
  "private": true,
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "build": "concurrently --names \"webpack,styleguide\" --prefix name \"npm run build:webpack\" \"npm run build:styleguide\"",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:styleguide": "styleguidist build",
    "ci": "concurrently --names \"jest,eslint,flow\" --prefix name \"npm run jest:ci\" \"npm run eslint:ci\" \"npm run flow\"",
    "eslint": "eslint .",
    "eslint:ci": "npm run eslint -- --quiet --format './node_modules/eslint-teamcity/index.js'",
    "eslintd": "eslint_d .",
    "flow": "flow check",
    "jest": "BABEL_ENV=test jest",
    "jest:ci": "npm run jest -- --teamcity",
    "jest:watch": "npm run jest -- --watch",
    "start": "node devServer.js",
    "start:acceptance": "BABEL_ENV=acceptance node acceptanceServer.js",
    "start:styleguide": "styleguidist server",
    "test": "concurrently --names \"jest,eslint,flow\" --prefix name \"npm run jest\" \"npm run eslint -- --quiet\" \"npm run flow\""
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-jest": "17.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-flow-react-proptypes": "0.16.0",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-react-jsx-self": "6.11.0",
    "babel-plugin-transform-react-jsx-source": "6.9.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "concurrently": "2.2.0",
    "cors": "2.8.1",
    "css-loader": "0.25.0",
    "elixir-smoothie": "2.X",
    "enzyme": "2.4.1",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "eslint-teamcity": "^1.3.1",
    "eslint_d": "4.2.1",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "flow-bin": "^0.38.0",
    "hard-source-webpack-plugin": "^0.3.8",
    "html-loader": "^0.4.4",
    "jest": "^18.0.0",
    "jest-teamcity-reporter": "^0.3.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.10.6",
    "markdown-loader": "^0.1.7",
    "phantomjs-prebuilt": "2.1.13",
    "react-addons-test-utils": "^15.4.1",
    "react-styleguidist": "^5.0.0-beta8",
    "react-svg-loader": "1.1.1",
    "react-test-renderer": "^15.4.1",
    "redux-saga-test": "https://github.com/aaronjensen/redux-saga-test.git#17b25ca",
    "style-loader": "0.13.1",
    "svgo": "^0.7.1",
    "svgo-loader": "1.1.2",
    "trackjs": "2.5.1",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "aphrodite": "1.0.0",
    "aws-sdk": "^2.7.10",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "fontfaceobserver": "^2.0.7",
    "invariant": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "2.15.1",
    "query-string": "^4.2.3",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dates": "3.1.1",
    "react-dom": "^15.4.1",
    "react-dropzone": "3.6.0",
    "react-helmet": "^3.3.1",
    "react-history": "^0.18.1",
    "react-motion": "^0.4.5",
    "react-redux": "^5.0.2",
    "react-router": "4.0.0-alpha.5",
    "react-router-match-async": "^0.0.4",
    "react-select": "1.0.0-rc.2",
    "redux": "3.6.0",
    "redux-form": "^6.4.3",
    "redux-form-submit-saga": "1.1.1",
    "redux-saga": "^0.13.0",
    "reflexbox": "2.2.2",
    "reselect": "^2.5.4",
    "ress": "1.1.1",
    "robox": "1.0.0-beta.8",
    "updeep": "^1.0.0",
    "xmlbuilder": "4.2.1"
  },
  "jest": {
    "resetMocks": true,
    "transformIgnorePatterns": [
      "node_modules/(?!react-dates)"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/client/config/jest/FileStub.js",
      "^[./a-zA-Z0-9$_-]+\\.css$": "<rootDir>/client/config/jest/CSSStub.js",
      "^[./a-zA-Z0-9$_-]+\\.md$": "<rootDir>/client/config/jest/MarkdownStub.js"
    },
    "moduleDirectories": [
      "<rootDir>/client",
      "<rootDir>/node_modules",
      "<rootDir>/node_modules/react-router/node_modules"
    ],
    "setupFiles": [
      "<rootDir>/client/app/polyfills.js",
      "<rootDir>/client/test/support/aphrodite.js",
      "<rootDir>/client/test/support/reflexbox.js"
    ],
    "testPathDirs": [
      "<rootDir>/client/app",
      "<rootDir>/client/test"
    ],
    "testEnvironment": "jsdom",
    "testResultsProcessor": "jest-teamcity-reporter"
  }
}
